[Unit]
Description=MDT GUI
After=network.target

[Service]
Type=simple
User=mdt
Group=mdt
WorkingDirectory=/home/mdt/mdt/mdt-gui/dtfront-main
ExecStartPre=/bin/sleep 10
ExecStart=yarn start
Restart=on-failure
RestartSec=10
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target
